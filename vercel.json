{
  "version": 2,
  "builds": [
    {
      "src": "server.js",   
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/contactme",       
      "dest": "/api/contactme"  
    },
    {
      "src": "/api/contactme",  
      "dest": "/server.js"       
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"      
    }
  ]
}
